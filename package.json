		"deploy",
		"test",
		"testing",
		"demo",
		"localtunnel"
    ],
    "engines": {
        "node": ">=0.10.0"
    },
    "dependencies": {
        "cli-config": "0.3.3",
        "shelljs": "0.2.6",
        "terminal-colors": "0.1.3",
        "connect-phonegap": "tohagan/connect-phonegap"
    }
